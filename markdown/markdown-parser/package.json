{
  "name": "@your-name/markdown-parser",
  "version": "1.0.0",
  "description": "A lightweight Markdown parser that generates AST tokens",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "markdown",
    "parser",
    "ast",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.19",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
